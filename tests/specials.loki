; SETP
(defclass Rocket
  ([x] (speed x))
  (lift_off []
    (print (+ "I'm flying @ "
              (. speed this)
              " speed")))
  (toString []
  	(+ "I'm a " 
  	    (. color this) 
  	    " rocket"))
  (color "red")
  (fuel 7))

(def r (new Rocket 7))
(print (. speed r))
(setp r speed 10)
(print (. speed r))