(defclass MyObject
  ([x] (obj x))
  (get_obj []
    (. obj this)))

(defclass MyThing
  ([x] (thing x))
  (get_thing []
    (. thing this)))

(defclass Rocket
  [MyObject MyThing]
  ([x] (super MyObject "obj")
       (super MyThing "thing")
       (speed x)
       &((print "eval-in-constr")
         (setp this foo "foo")
         (for [x (range 10)]
           (print x))))
  (lift_off []
    (print (+ "I'm flying @ "
              (. speed this)
              " speed")))
  (toString []
  	(+ "I'm a "
  	    (. color this)
  	    " rocket"))
  (color "red")
  (fuel 7))

(def r (new Rocket "5"))
(print (. foo r))
(print (. color r))
(print (. speed r))
(print (. toString r))
(print (. fuel r))
(. lift_off r)
(print (. get_obj r))
(print (. thing r))

(defclass NoConstrOrSuperClasses
  (nope "nope"))

(def no (new NoConstrOrSuperClasses ))
(print (. nope no))
